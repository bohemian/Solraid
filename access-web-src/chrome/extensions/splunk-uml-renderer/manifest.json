{
  "key": "",
  "manifest_version": 3,
  "name": "Splunk UML Renderer",
  "version": "0.0.4",
  "description": "Renders UML in Splunk logs",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://splunk.paas-inf.net/*"
      ],
      "js": [
        "mermaid.js",
        "content.js"
      ]
    }
  ]
}
